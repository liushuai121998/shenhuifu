{
  "package": "com.shenhuifu.package",
  "name": "一同神回复",
  "versionName": "1.0.0",
  "versionCode": "1",
  "minPlatformVersion": "1060",
  "icon": "/Common/logo.png",
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    },
    { "name": "system.fetch" },
    { "name": "system.clipboard" },
    {"name":"service.ad"}
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "config": {
    "logLevel": "off"
  },
  "router": {
    "entry": "Home",
    "pages": {
      "Home": {
